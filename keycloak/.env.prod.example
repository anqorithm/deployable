POSTGRES_DATA_PATH=keycloak

# Keycloak
KEYCLOAK_CONTAINER_NAME=keycloak
KEYCLOAK_HOSTNAME_STRICT=false
KEYCLOAK_HOSTNAME_STRICT_HTTPS=false
