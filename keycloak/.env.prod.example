# Nginx
NGINX_CONTAINER_NAME=nginx
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443
NGINX_CONF_PATH=./nginx/conf.d

# Certbot
CERTBOT_CONTAINER_NAME=certbot
CERTBOT_CONF_PATH=./certbot/conf
CERTBOT_WWW_PATH=./certbot/www

# Network
KEYCLOAK_NETWORK_NAME=keycloak-network